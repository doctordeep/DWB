{
  "name": "donaldboulton",
  "version": "4.12.0",
  "description": "Donald Boulton theme npm server scripts",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/donaldboulton/DWB.git"
  },
  "keywords": [
    "jekyll",
    "node",
    "babel",
    "Donald Boulton",
    "Web Push API",
    "Web Notifications API",
    "Service Worker",
    "Firebase Cloud Messaging",
    "FCM"
  ],
  "author": "Donald Boulton",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/donaldboulton/issues"
  },
  "homepage": "https://github.com/donaldboulton/#readme",
  "scripts": {
    "start": "npm run-script clean:project && webpack --env=dev --progress --profile --colors && concurrently \"webpack-dev-server --env=dev\" \"bundle exec jekyll serve\"",
    "build:optim": "npm run-script clean:project && cross-env NODE_ENV=production webpack --env=prod --progress --profile --colors && webpack --env=optim  --progress --profile --colors && cross-env JEKYLL_ENV=production bundle exec jekyll build",
    "build:pwa:optim": "npm run-script clean:project && cross-env NODE_ENV=production webpack --env=pwa --progress --profile --colors && webpack --env=optim  --progress --profile --colors && cross-env JEKYLL_ENV=pwa bundle exec jekyll build && sw-precache --config=config/sw.config.js --verbose",
    "build": "npm run-script clean:project && cross-env NODE_ENV=production webpack -p --env=prod --progress --profile --colors && cross-env JEKYLL_ENV=production bundle exec jekyll build",
    "build:pwa": "npm run-script clean:project && cross-env NODE_ENV=production webpack -p --env=pwa --progress --profile --colors && cross-env JEKYLL_ENV=pwa bundle exec jekyll build && sw-precache --config=config/sw.config.js --verbose",
    "serve:dist": "http-server _site",
    "clean:project": "rimraf ./_site ./assets"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.24.1",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "curl": "^0.1.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "node-sass": "^4.5.3",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "sw-precache": "^5.2.1",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-plugin-critical": "^1.0.0",
    "webpack-pwa-manifest": "^3.1.6"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "anchor-js": "^4.1.1",
    "babel-preset-react-app": "^3.1.0",
    "compression-webpack-plugin": "^1.1.10",
    "default-passive-events": "^1.0.10",
    "express": "^4.16.3",
    "file-upload-with-preview": "^2.1.1",
    "firebase": "^5.4.1",
    "google-auth-library": "^1.6.1",
    "googleapis": "^33.0.0",
    "imports-loader": "^0.8.0",
    "jquery-smooth-scroll": "^2.2.0",
    "lazysizes": "^4.1.2",
    "mailgun-js": "^0.21.0",
    "md5": "^2.2.1",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "netlify-identity-widget": "^1.4.14",
    "node-fetch": "^2.2.0",
    "normalize.css": "^7.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "svg4everybody": "^2.1.9",
    "utf-8-validate": "^5.0.1"
  }
}
